{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@main/*": [
        "./src/main/*"
      ],
      "@common/*": [
        "./src/common/*"
      ]
    }
  },
  "include": [
    "src/main",
    "rsbuild.config.ts"
  ]
}